{
	"name": "wikipediaplus",
	"version": "1.1.21",
	"description": "WikipediaPlus is an extension that allows you to quickly and efficiently access all relevant links on the page you're visiting.",
	"manifest_version": 3,
	"icons": {
		"16": "../icon-16.png",
		"32": "../icon-32.png",
		"48": "../icon-48.png",
		"96": "../icon-96.png",
		"128": "../icon-128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://wikipedia.org/*",
				"https://*.wikipedia.org/*"
			],
			"js": [
				"../content.js"
			],
			"css": [
				"../content.css"
			],
			"run_at": "document_end"
		}
	]
}
